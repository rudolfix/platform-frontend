  "license": "MIT",
  "scripts": {
    "build:prod": "(cp -n ./app/config/.env.example .env || true) && NODE_ENV=production webpack --config ./webpack/webpack.config.prod.js",
    "build:commit-deployment": "cp ./app/config/.env.common .env && cat ./app/config/.env.commit >> .env | webpack --config ./webpack/webpack.config.prod.js",
    "build:prod:e2e": "cp ./app/config/.env.common .env && cat ./app/config/.env.e2e >> .env | webpack --config ./webpack/webpack.config.prod.js",
    "build:prod:e2e:io": "cp ./app/config/.env.common .env && cat ./app/config/.env.e2e.io >> .env | webpack --config ./webpack/webpack.config.prod.js",
    "build:translate": "(cp -n ./app/config/.env.example .env || true) && NODE_ENV=production && NF_ENABLE_TRANSLATE_OVERLAY=1 webpack --config ./webpack/webpack.config.prod.js",
    "bundle-analyzer": "(cp -n ./app/config/.env.example .env || true) && NODE_OPTIONS=--max_old_space_size=9192 NODE_ENV=production webpack --config ./webpack/webpack.config.bundle-analyzer.js",
    "build:prod:prerender": "./scripts/prerender.sh",
    "bundlewatcher": "./setup-bundlewatcher.sh",
    "contract-artifacts:check": "node ./scripts/getShasFromHealthCheck.js --check",
    "contract-artifacts:generate": "node ./scripts/generate-contracts.js",
    "contract-artifacts:update": "node ./scripts/getShasFromHealthCheck.js",
    "deduplicate": "yarn-deduplicate",
    "deploy-storybook": "storybook-to-ghpages --out=storybook-static",
    "format": "prettier --list-different '**/*.{ts,tsx,js,json,md,gql}'",
    "format:fix": "prettier --write '**/*.{ts,tsx,js,json,md,gql}'",
    "ganache": "./scripts/ganache-cli.sh",
    "intl:extract": "./scripts/extract-intl-messages.sh",
    "intl:extract:check": "./scripts/extract-intl-messages.sh --check",
    "lint": "yarn format && yarn tslint && yarn sasslint && yarn tsc && yarn intl:extract:check",
    "lint:fix": "yarn contract-artifacts:update && concurrently -p \"[{name}]\" -n \"TSLINT/PRETTIER,SASS/TSC/INTL\" -c \"bgYellow.bold,bgMagenta.bold\" \"yarn tslint:fix && yarn format:fix\" \"yarn sasslint && yarn tsc && yarn intl:extract\"",
    "lint:fix:staged": "concurrently -p \"[{name}]\" -n \"TSC,LINT-STAGED\" -c \"bgYellow.bold,bgMagenta.bold\" \"yarn tsc\" \"lint-staged\"",
    "prepare": "git submodule update --init --recursive && yarn contract-artifacts:generate",
    "getArtifacts:check": "node ./scripts/getShasFromHealthCheck.js --check",
    "sasslint": "sass-lint -c .sass-lint.yml -v -q",
    "start": "(cp -n ./app/config/.env.example .env || true) && NODE_ENV=development webpack-dev-server --config ./webpack/webpack.config.dev.js",
    "start:remote": "(cp -n ./app/config/.env.example .env || true) && NODE_ENV=development webpack-dev-server --config ./webpack/webpack.config.dev-remote.js",
    "start:remote:vm": "(cp -n ./app/config/.env.example .env || true) && NODE_ENV=development && NF_VM_CONNECT=1 webpack-dev-server --config ./webpack/webpack.config.dev.js",
    "start:remote:serve-on-network": "(cp -n ./app/config/.env.example .env || true) && NODE_ENV=development && NF_SERVE_ON_NETWORK=$(eval network private_ip) webpack-dev-server --config ./webpack/webpack.config.dev-remote-external.js",
    "start:translate": "(cp -n ./app/config/.env.example .env || true) && NODE_ENV=development && NF_ENABLE_TRANSLATE_OVERLAY=1 webpack-dev-server --config ./webpack/webpack.config.translate.js",
    "storybook": "(cp -n ./app/config/.env.storybook .env || true) && start-storybook -p 9001 -c .storybook -s ./app/assets",
    "storybook:screenshots": "(cp -n ./app/config/.env.storybook .env || true) && storybook-chrome-screenshot --debug --inject-files ./.storybook/disables.js -p 9001 -c .storybook --browser-timeout 999999 -s ./app/assets",
    "test:browser": "TS_NODE_PROJECT=tsconfig.spec.json mocha --require ts-node/register --require jsdom-global/register --require ./test/setupBrowserTests.ts './test/setupTestsHooks.ts' './app/**/*.spec.ts?(x)'",
    "test:browser:watch": "TS_NODE_PROJECT=tsconfig.spec.json mocha --require ts-node/register --require jsdom-global/register --require ./test/setupBrowserTests.ts --watch --watch-extensions ts,tsx './test/setupTestsHooks.ts' './app/**/*.spec.ts?(x)'",
    "test:e2e": "./e2e/setup-e2e.sh",
    "test:e2e:chrome": "./e2e/setup-docker-e2e.sh",
    "test:e2e:cypress": "CYPRESS_RETRIES=2 cypress run --config testFiles=\"**/*.spec.e2e.ts\"",
    "test:e2e:cypress:chrome": "CYPRESS_RETRIES=2 cypress run --config testFiles=\"**/*.spec.e2e.chrome.ts\" --browser chrome",
    "test:e2e:cypress:record": "echo build id $CIRCLE_BUILD_NUM$(echo node) && CYPRESS_RETRIES=2 cypress run --config testFiles=\"**/*.spec.e2e.ts\" --record --parallel --group nodeTests --ci-build-id $CIRCLE_BUILD_NUM$(echo node)",
    "test:e2e:cypress:record:chrome": "CYPRESS_RETRIES=2 cypress run --config testFiles=\"**/*.spec.e2e.ts\" --record --parallel --browser chrome",
    "test:e2e:cypress:io": "echo build id $CIRCLE_BUILD_NUM$(echo io) && cypress run --config testFiles=\"**/*.spec.e2e.io.ts\" --record --parallel --group io --ci-build-id $CIRCLE_BUILD_NUM$(echo io)",
    "test:e2e:dev": "cypress open --config testFiles=\"**/*.spec.e2e.*\"",
    "test:node": "TS_NODE_PROJECT=tsconfig.spec.json mocha --require ts-node/register --require ./test/setupTests.ts './app/**/*.spec.node.ts?(x)'",
    "tsc": "concurrently -p \"[{name}]\" -n \"TSC:APP,TSC:E2E\" -c \"bgYellow.bold,bgMagenta.bold\" \"yarn tsc:app\" \"yarn tsc:e2e\"",
    "tsc:app": "tsc --noEmit --project ./tsconfig.json",
    "tsc:e2e": "tsc --noEmit --project ./tsconfig.e2e.json",
    "tslint": "concurrently -p \"[{name}]\" -n \"APP,E2E\" -c \"bgGreen.bold,bgCyan.bold\" \"yarn tslint:app\" \"yarn tslint:e2e\"",
    "tslint:app": "NODE_OPTIONS=--max_old_space_size=9192 tslint --project ./tsconfig.json --format stylish",
    "tslint:e2e": "tslint --project ./tsconfig.e2e.json --format stylish",
    "tslint:fix": "concurrently -p \"[{name}]\" -n \"APP,E2E\" -c \"bgGreen.bold,bgCyan.bold\" \"yarn tslint:app --fix\" \"yarn tslint:e2e --fix\"",
    "postinstall": "patch-package"
  },
