{
  "name": "@neufund/wallet",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean:manual": "react-native clean-project",
    "clean": "react-native clean-project-auto",
    "install-pods": "cd ios && pod install && cd ..",
    "format": "prettier --list-different '**/*.{ts,tsx,js,json,md,gql}'",
    "format:fix": "prettier --write '**/*.{ts,tsx,js,json,md,gql}' --loglevel warn",
    "start:android": "react-native run-android",
    "start:android:release": "react-native run-android --variant release",
    "start": "react-native start -- --reset-cache",
    "start:ios": "react-native run-ios",
    "start:ios:device": "react-native run-ios --device",
    "start:ios:release": "react-native run-ios --configuration Release",
    "start:devtools": "react-devtools",
    "tsc": "concurrently -p '[TSC:{name}]' -n 'APP,UNIT,E2E' -c 'bgYellow.bold,bgGreen.bold,bgMagenta.bold' 'yarn tsc:app' 'yarn tsc:unit' 'yarn tsc:e2e'",
    "tsc:app": "tsc --project ./tsconfig.json",
    "tsc:e2e": "tsc --project ./tsconfig.e2e.json",
    "tsc:unit": "tsc --project ./tsconfig.unit.json",
    "lint": "concurrently -p '[LINT:{name}]' -n 'FORMAT,TSC,ESLINT' -c 'bgYellow.bold,bgGreen.bold,bgRed.bold' 'yarn format' 'yarn tsc' 'yarn eslint'",
    "lint:fix": "concurrently -p '[LINT:{name}]' -n 'FORMAT,TSC,ESLINT' -c 'bgYellow.bold,bgGreen.bold,bgRed.bold' 'yarn format:fix' 'yarn tsc' 'yarn eslint:fix'",
    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "eslint:fix": "yarn eslint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "e2e:build:ios-debug": "detox build --configuration ios.sim.debug",
    "e2e:build:ios-release": "detox build --configuration ios.sim.release",
    "e2e:build:android-debug": "detox build --configuration android.emu.debug",
    "e2e:build:android-release": "detox build --configuration android.emu.release",
    "e2e:test:ios-debug": "detox test --configuration ios.sim.debug",
    "e2e:test:ios-release": "detox test --configuration ios.sim.release",
    "e2e:test:android-debug": "detox test --configuration android.emu.debug",
    "e2e:test:android-release": "detox test --configuration android.emu.release",
    "e2e:ios-debug": "yarn e2e:build:ios-debug && yarn e2e:test:ios-debug",
    "e2e:ios-release": "yarn e2e:build:ios-release && yarn e2e:test:ios-release",
    "e2e:android-debug": "yarn e2e:build:android-debug && yarn e2e:test:android-debug",
    "e2e:android-release": "yarn e2e:build:android-release && yarn e2e:test:android-release",
    "storybook": "(adb reverse tcp:7007 tcp:7007 || true) && start-storybook",
    "storybook:ios": "ENVFILE=./storybook/.env.storybook react-native run-ios",
    "storybook:ios:release": "ENVFILE=./storybook/.env.storybook react-native run-ios --configuration Release",
    "storybook:android": "ENVFILE=./storybook/.env.storybook react-native run-android",
    "storybook:android:release": "ENVFILE=./storybook/.env.storybook react-native run-android --variant release",
    "contract-artifacts:generate": "node ./scripts/generate-contracts.js",
    "postinstall": "patch-package",
    "prepare": "yarn contract-artifacts:generate",
    "prestorybook": "rnstl"
  },
  "dependencies": {
    "@expo/react-native-action-sheet": "^3.6.0",
    "@neufund/sagas": "^1.0.0",
    "@neufund/shared-modules": "^1.0.0",
    "@neufund/shared-utils": "^1.0.0",
    "@react-native-community/async-storage": "^1.8.0",
    "@react-native-community/masked-view": "^0.1.6",
    "@react-native-firebase/app": "^6.4.0",
    "@react-native-firebase/messaging": "^6.4.0",
    "@react-navigation/bottom-tabs": "^5.2.7",
    "@react-navigation/native": "^5.1.6",
    "@react-navigation/stack": "^5.2.11",
    "@walletconnect/react-native": "^1.0.0-beta.47",
    "bignumber.js": "^5.0.0",
    "ethers": "^4.0.45",
    "eventemitter2": "^6.0.0",
    "formik": "^2.1.3",
    "fuse.js": "^5.1.0",
    "inversify": "^4.10.0",
    "memory-cache": "^0.2.0",
    "process": "^0.11.10",
    "react": "16.9.0",
    "react-native": "0.61.5",
    "react-native-bootsplash": "^2.1.0",
    "react-native-camera": "^3.19.0",
    "react-native-config": "^0.12.0",
    "react-native-crypto": "^2.2.0",
    "react-native-dev-menu": "^4.0.2",
    "react-native-device-info": "^5.5.5",
    "react-native-gesture-handler": "^1.5.6",
    "react-native-keychain": "^6.0.0",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-notifications": "^3.1.2",
    "react-native-permissions": "^2.0.9",
    "react-native-qrcode-scanner": "^1.3.1",
    "react-native-randombytes": "^3.0.0",
    "react-native-reanimated": "^1.7.0",
    "react-native-safe-area-context": "^0.7.2",
    "react-native-safe-area-view": "^1.0.0",
    "react-native-screens": "^2.0.0-beta.2",
    "react-native-svg": "^12.0.3",
    "react-native-svg-transformer": "^0.14.3",
    "react-native-webview": "^9.4.0",
    "react-redux": "^6.0.0",
    "react-intl": "^4.5.1",
    "recompose": "^0.30.0",
    "redux": "^4.0.1",
    "reflect-metadata": "^0.1.10",
    "reselect": "^4.0.0",
    "stream-browserify": "^2.0.2",
    "tslib": "^1.10.0",
    "vm-browserify": "^1.1.2",
    "yup": "^0.28.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/runtime": "^7.9.2",
    "@neufund/eslint-config": "^1.0.0",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addon-ondevice-actions": "^5.3.18",
    "@storybook/addon-ondevice-knobs": "^5.3.18",
    "@storybook/addon-ondevice-notes": "^5.3.18",
    "@storybook/react-native": "^5.3.18",
    "@storybook/react-native-server": "^5.3.18",
    "@testing-library/react-native": "^5.0.3",
    "@types/detox": "^14.5.2",
    "@types/jest": "^25.1.2",
    "@types/memory-cache": "^0.2.1",
    "@types/react-native": "^0.61.20",
    "@types/react-native-permissions": "^2.0.0",
    "@types/react-redux": "^6.0.11",
    "@types/react-test-renderer": "16.9.1",
    "@types/redux-saga": "^0.10.5",
    "@types/reflect-metadata": "^0.1.0",
    "@types/yup": "^0.26.32",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-parameter-decorator": "^1.0.16",
    "concurrently": "^5.1.0",
    "detox": "^16.2.0",
    "eslint": "^6.8.0",
    "jest": "^25.1.0",
    "metro-config": "^0.56.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "prettier": "^2.0.4",
    "react-devtools": "^3.6.3",
    "react-native-clean-project": "^3.3.0",
    "react-native-storybook-loader": "^1.8.1",
    "react-test-renderer": "16.9.0",
    "ts-generator": "^0.0.8",
    "ts-jest": "^25.2.1",
    "typechain": "^1.0.5",
    "typechain-target-ethers": "^1.0.4",
    "typedoc": "^0.16.10",
    "typescript": "^3.8.3"
  },
  "workspaces": {
    "nohoist": [
      "**/react-native",
      "**/react-native/**",
      "**/react-native-*",
      "**/react-native-*/**",
      "**/@react-native-firebase/**",
      "**/detox",
      "**/yup",
      "**/detox/**",
      "**/@types/detox",
      "**/@types/detox/**",
      "**/@types/jest",
      "**/@types/jest/**",
      "**/jest",
      "**/jest/**",
      "**/@neufund/eslint-config",
      "**/@neufund/eslint-config/**"
    ]
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/wallet.app",
        "build": "xcodebuild -workspace ios/wallet.xcworkspace -scheme wallet -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "device": {
          "type": "iPhone X"
        }
      },
      "ios.sim.release": {
        "binaryPath": "ios/build/Build/Products/Release-iphonesimulator/wallet.app",
        "build": "xcodebuild -workspace ios/wallet.xcworkspace -scheme wallet -configuration Release -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "device": {
          "type": "iPhone X"
        }
      },
      "android.emu.debug": {
        "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
        "build": "cd android && ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug && cd ..",
        "type": "android.emulator",
        "device": {
          "avdName": "Nexus_5X_API_29_x86"
        }
      },
      "android.emu.release": {
        "binaryPath": "android/app/build/outputs/apk/release/app-release.apk",
        "build": "cd android && ./gradlew assembleRelease assembleAndroidTest -DtestBuildType=release && cd ..",
        "type": "android.emulator",
        "device": {
          "avdName": "Nexus_5X_API_29_x86"
        }
      }
    },
    "test-runner": "jest",
    "runner-config": "./tests/e2e/jest.config.json"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": "./app/components",
      "pattern": "**/*.stories.tsx",
      "outputFile": "./storybook/storyLoader.js"
    }
  }
}
